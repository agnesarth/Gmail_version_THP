<tr id="<%= email.id %>">
  <th scope="row">
  <%= email.id %></th>
  <td>
    <% if email.read == true %>
      <%= link_to email.object, email_path(email), remote: true %>
    <% else %>
      <strong><%= link_to email.object, email_path(email), remote: true %></strong>
    <% end %>
  </td>
</tr>